<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="WorkTool.Core.Modules.SmsClub.Views.ControlPanelView"
             Name="ControlPanelView">
    <Interaction.Behaviors>
        <EventTriggerBehavior EventName="Loaded" SourceObject="ControlPanelView" Actions="">
            <InvokeCommandAction Command="{Binding LoadedCommand}"/>
        </EventTriggerBehavior>
    </Interaction.Behaviors>
    <ItemsControl Items="{Binding Originators}"></ItemsControl>
</UserControl>
